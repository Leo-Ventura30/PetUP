{% extends "dashboard.njk" %} {% block body2 %}

<ul>
  {% for schedule in sched %}
  <li>
    <h1>Horá  rio: {{ schedule.date.day }} - {{ schedule.date.hour }}</h1>
    <h2>Endereço: {{ schedule.location }}</h2>
    <h3>Tipo: {{ schedule.type }}</h3>
    <h3>Valor: {{ schedule.value }}</h3>
    <h4>Status: {{ stats[schedule.status] }}</h4>
    <a href="/dashboard/home/schedules/{{ schedule.id }}">Detalhes</a>
  </li>
  {% endfor %}
</ul>

{% endblock %}